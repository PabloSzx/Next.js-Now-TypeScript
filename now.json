{
  "version": 2,
  "name": "nextjs",
  "builds": [{ "src": "next.config.js", "use": "@now/next" }],
  "env": {
    "IS_NOW": "true"
  },
  "routes": [
    { "src": "/asd", "dest": "/asd" },
    { "src": "/test", "dest": "/asd/zxc" },
    { "src": "/other", "dest": "/" },
    { "src": "/", "dest": "/asd/zxc" }
  ]
}
